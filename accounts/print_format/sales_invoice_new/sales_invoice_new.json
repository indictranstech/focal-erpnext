 "html": "<div class=\"letter-head\">\n{{ letter_head }}\n</div>\n<div class=\"print-heading\">\n  <h3>Sales Invoice</h3>\n</div>\n<div class=\"row\">\n  <div class=\"col-xs-6\">\n    <div class=\"row\">\n      <div class=\"col-xs-5 text-right\" ><b>Customer Name:</b></div>\n      <div class=\"col-xs-7\">{{ doc.customer or \"\" }}</div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-xs-5 text-right\"><b>Address:</b></div>\n      <div class=\"col-xs-7\">{% set testing = doc.address_display or \"\" %}{% set another = \"<br>\".join(testing.split('<br>')) %}{{ another }}</div>\n    </div>\n  </div>\n  <div class=\"col-xs-6\">\n     <div class=\"row\">\n      <div class=\"col-xs-5 text-right\"><b>Customer Ref:</b></div>\n      <div class=\"col-xs-7\">{{ doc.customer_ref_no or \" \" }}</div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-xs-5 text-right\"><b>Date:</b></div>\n      <div class=\"col-xs-7\">{{ doc.get_formatted(\"posting_date\") or \"\" }}</div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-xs-5 text-right\"><b>Payment Due Date:</b></div>\n      <div class=\"col-xs-7\">{{ doc.get_formatted(\"due_date\") or \"\"  }}</div>\n    </div>\n   </div>\n</div>\n<br>\n<div class=\"row\">\n  <div class=\"col-xs-12\">\n    <table class=\"table table-bordered\">\n      <tbody>\n        <tr>\n          <th style=\"width:10% text-align:center\" >S/N</th>\n          <th style=\"width:30% text-align:center\" >Item</th>\n          <th style=\"width:10%;text-align:center\" >Qty</th>\n          <th style=\"width:25%;text-align:center\" >Rate</th>\n          <th style=\"width:25%;text-align:center\" >Amount</th>\n        </tr>\n        {% set count=1 %}\n        {%- for row in doc.entries -%}\n        <tr>\n          <td style=\"width:10% text-align:left\" >{{ count }}</td>\n          <td style=\"width:30% text-align:left\" ><b>{{ row.item_code }}</b><br>{{ row.item_name }}<br>\n          <b>Part Number:</b>{{ row.part_number }}</td>\n          <td style=\"width:10%;text-align:left\" >{{ row.qty }}<p>{{ row.stock_uom }}</p></td>\n          <td style=\"width:25%;text-align:right\" >{{ row.get_formatted(\"rate\", doc) }}</td>\n          <td style=\"width:25%;text-align:right\" >{{ row.get_formatted(\"amount\", doc) }}</td>\n        </tr>\n        {% set count=count+1 %}\n        {%- endfor %}  \n      </tbody>  \n    </table>   \n  </div>\n</div>\n<div class=\"row\">\n  <div class=\"col-xs-6\"></div>\n  <div class=\"col-xs-6\">\n    <div class=\"col-xs-5 text-right\"><b>Net Total</b></div>\n    <div class=\"col-xs-7 text-right\">{{ doc.get_formatted(\"net_total_export\") }}</div>\n  </div>\n</div>\n{% set count=1 %}\n{%- for row in doc.other_charges -%}\n<div class=\"row\">\n  <div class=\"col-xs-6\"></div>\n  <div class=\"col-xs-6\">\n    <div class=\"col-xs-5 text-right\"><b>{{ row.description }}</b></div>\n    <div class=\"col-xs-7 text-right\">{{ row.get_formatted(\"tax_amount\", doc) }}</div>\n  </div>\n</div>\n{% set count=count+1 %}\n{%- endfor %}  \n<div class=\"row\">\n  <div class=\"col-xs-6\"></div>\n  <div class=\"col-xs-6\">\n    <div class=\"col-xs-5 text-right\"><b>Grand Total</b></div>\n    <div class=\"col-xs-7 text-right\">{{ doc.get_formatted(\"grand_total_export\") }}</div>\n  </div>\n</div>\n<div class=\"row\">\n  <div class=\"col-xs-6\"></div>\n  <div class=\"col-xs-6\">\n    <div class=\"col-xs-5 text-right\"><b>In Words</b></div>\n    <div class=\"col-xs-7 text-right\">{{ doc.in_words_export }}</div>\n  </div>\n</div>\n{%- if doc.terms %}\n<div class=\"row\">\n  <div class=\"col-xs-12 text-left\"><b>Terms and Conditions</b></div>\n</div>\n<div class=\"row\">\n  <div class=\"col-xs-12 text-left\">{{ doc.terms }}</div>\n</div>\n{%- endif %}", 
