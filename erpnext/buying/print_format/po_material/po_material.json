 "html": "{{ letter_head }}\n</br>\n</br>\n<html> \n      <body>\n        <table width=100%>\n              <tr width=100%>\n                  <td width=50%>\n                      <table width=100% style=\"line-height:1;\">\n                          <tr width=100%>\n                             <td width=30%><b>Vendor:</b></td>\n                             <td width=70%>{{ doc.supplier or \" \" }}</td>\n                          </tr>\n                          <tr width=100%>\n                             <td width=30%><b>Address:</b></td>\n                             <td width=70%>{{ doc.address_display or \" \" }}\n                          </tr>\n                      </table>\n                  </td>\n                  <td width=50%>\n                       <table width=100% style=\"line-height:1;\">\n                           <tr width=100%>\n                               <td width=30%><b>PO No:</b></td>\n                               <td width=70%>{{ doc.po_no or \" \" }}</td>\n                           </tr>\n                           <tr width=100%>\n                               <td width=30%><b>Date:</b></td>\n                               <td width=70%>{{ doc.get_formatted(\"date\") }}</td> \n                           </tr>\n                           <tr width=100%>\n                                <td width=40%><b>Requested By:</b></td>\n                                <td width=60%>{{ doc.requested_by or \" \" }}\n                           </tr>\n                           <tr width=100%>\n                                 <td width=40%><b>Currency:</b></td>\n                                 <td width=60%>{{ doc.currency or \" \" }}</td>\n                           </tr>\n                       </table>\n                  </td>\n              </tr>\n          </table>\n</br>\n</br>\n<table border=\"1\" width=100% class=table-bordered style=\"text-align:center;\">\n    <tr>\n        <th rowspan=\"2\" style=\"text-align:center;\">S/N</th>\n          <th rowspan=\"2\" style=\"text-align:center;\">Description</th>\n        <th colspan=\"3\" style=\"text-align:center;\">Dimension</th>\n         <th rowspan=\"2\" style=\"text-align:center;\">Qty</th>\n          <th rowspan=\"2\" style=\"text-align:center;\">Req. Date</th>\n           <th rowspan=\"2\" style=\"text-align:center;\">Unit Price</th>\n          <th rowspan=\"2\"style=\"text-align:center;\">Total Price</th>\n          \n    </tr>\n    <tr>\n        <th style=\"text-align:center;\">OD</td>\n        <th style=\"text-align:center;\">ID</td>\n        <th style=\"text-align:center;\">LG</td>\n    </tr>\n       {%- for row in doc.raw_material_details -%}\n                 <tr width=100%>\n                 <td style=\"text-align:center;\">{{ row.idx or \" \"}}</td>\n                <td width=37% style=\"text-align:left;\">\n               {%-if row.spec -%}\n                  <b>Spec:   </b>  {{ row.spec or \" \" }}</br>\n                {%- endif -%}\n                {%-if row.type -%}\n                <b>Type:   </b>  {{ row.type or \" \" }}</br>\n                {%- endif -%}\n                {%-if row.job_order -%}\n                 <b>Job Order:  </b> {{ row.job_order  or \" \" }}</br>\n                {%- endif -%}\n                {%-if row.part_name -%}\n                 <b>Part Name:  </b>  {{ row.part_name or \" \" }}</br>\n                {%- endif -%}\n                {%-if row.part_no  -%}\n                  <b>Part No:   </b>  {{ row.part_no or \" \"  }}</br>\n                {%- endif -%}\n                 {%-if row.drawing_no  -%}\n                 <b>Drawing No:   </b> {{ row.drawing_no or \" \"  }}</br>\n                {%- endif -%}\n                 {%-if row.po_number  -%}\n              <b>PO Number:   </b>            {{ row.po_number or \" \" }}</br>\n                {%- endif -%}\n               {%-if row.batch_no  -%}\n              <b>Batch No:   </b>       {{ row.batch_no or \" \" }}</br>\n                {%- endif -%}\n               {%-if row.quote_ref  -%}\n              <b>Quote Ref:   </b>      {{ row.quote_ref or \" \" }}</br>\n                {%- endif -%}\n               </td>\n              <td style=\"text-align:center;\">{{ row.od or \" \" }} {{ row.od_uom }}</td>\n               <td style=\"text-align:center;\">{{ row.id or \" \" }} {{ row.id_uom }}</td>\n              <td style=\"text-align:center;\">{{ row.lg or \" \" }} {{ row.lg_uom }}</td>\n               <td style=\"text-align:center;\">{{ row.qty or \" \" }}\n                <td width=10% style=\"text-align:center;\">{{ row.required_date or \" \"}}</td>\n               <td width=10% style=\"text-align:center;\">{{ frappe.db.get_value(\"Currency\", doc.currency, \"symbol\") }} {{ row.get_formatted(\"unit_cost\", doc) }}</td>\n             <td width=10% style=\"text-align:center;\">{{ frappe.db.get_value(\"Currency\", doc.currency, \"symbol\") }} {{ row.price|int * row.qty|int }}</td> \n                 </tr>\n        \n                 {%- endfor -%}\n</table>\n</br>\n</br>\n    \n    <table width=100%>\n         <tr width=100%>\n             <td width=30%></td>\n             <td width=30%></td>\n             <td width=20%><b>Net Total :</b></td>\n             <td width=20%> {{ frappe.db.get_value(\"Currency\", doc.currency, \"symbol\") }} {{ doc.get_formatted(\"net_total\", doc) }}</td>\n         </tr>\n    </table>\n   </br>\n   </br> \n   \n    {%-if doc.remarks  -%}\n      <table width=100%>\n          <tr width=100%>\n               <td width=15%><b>Remarks</b></td>\n               <td width=85%>{{ doc.remarks or \" \" }}</td>\n         </tr>\n      </table>\n {%- endif -%}\n</br>\n</br>\n{%-if doc.terms  -%}\n         <table width=100%>\n             <tr width=100%>\n               <th width=100%><b>Terms and condition</b><th>\n             </tr>\n            <tr width=100%>\n               <td width=100%>{{ doc.terms or \" \" }}</td>\n            </tr>\n         </table>\n {%- endif -%}\n      </body>\n</html> "