"15%\">{{ count }}</th>\n                           {% endif %}\n                        {% set count = count+1 %} \n                        {% endfor %}\n               </tr>\n               {%- for r in doc.measurements %}\n                <tr>\n\t\t\t<td style=\"text-align:center;\">{{ r.idx }}</td>\n\t\t\t<td style=\"text-align:center;width:12%\">{{ r.dimension or \"\" }}</td>\n\t\t\t<td style=\"text-align:center;width:8%\">{{ r.tolerance_to or \"\"}}</td>\n                        <td style=\"text-align:center;width:8%\">{{ r.tolerance_from or \"\"}}</td>\n                        {% if r.actual_mesurement %}\n                        {% set a=frappe.get_doc('Actual Mesurement',r.actual_mesurement) %}\n                        {% set w=a.get('mesurement_values') %}\n                        {%- for i in range(0,5) %}\n\t\t\t     {% if count <= doc.quantity|int %}\n                               <td style=\"text-align:center;word-wrap:break-word\">{{ w[count-1].value or \"\" }}</td>\n                             {% endif %}\n                          {% set count = count+1 %}\n                        {% endfor %}\n                        {% else %}\n                            {%- for i in range(0,5) %}\n\t\t\t      {% if count <= doc.quantity|int %}\n                                <td style=\"text-align:center;word-wrap:break-word\" width=\"15%\">{{ w[count-1].value or \"\" }}</td>\n                              {% endif %}\n                              {% set count = count+1 %}\n                            {% endfor %}\n                        {% endif %}\n\t\t</tr>\n                {% endfor %}\n                <tr>\n                         <th colspan=\"4\" style=\"text-align:center;\">Heat</th>\n                         {% set h=doc.get('heat_no_details') %}\n                         {%- for i in range(0,5) %}\n            \t\t    {% if count <= doc.quantity|int %}\n                               <td style=\"text-align:center;word-wrap:break-word\" width=\"15%\">{{ h[count-1].heat_no or \"\" }}</td>\n                             {% endif %}\n                          {% set count = count+1 %}\n                        {% endfor %}\n                </tr>\n                 \n                {% set count = count+5 %}\n             \n\t\t\n\t</tbody>\n</table>\n{% endfor -%}\n\t\n<div>\n       <table width=100%>\n              <tbody>\n                     <tr width=100%>\n                         <td width=20%><b>Inspected By :</b></td>\n                         <td width=30%>{{ doc.inspector_name or \"\" }}</td>\n                         <td width=20%><b>Approved By :</b></td>\n                         <td width=30%><hr></td>\n                    </tr>\n                  \n                  <tr width=100% height=20px>\n                          <td width=20%><b>Signature :</b></td>\n                         <td width=30%><hr></td>\n                         <td width=20%><b>Signature :</b></td>\n                         <td width=30%><hr></td>\n                    </tr>\n                    <tr width=100% height=20px>\n                          <td width=20%><b>Date :</b></td>\n                         <td width=30%>{{ doc.date or \"\" }}</td>\n                         <td width=20%><b>Company Stamp :</b></td>\n                         <td width=30%><hr></td>\n                    </tr>\n              </tbody>\n       </table>\n</div>\n\n\n</html>", 
