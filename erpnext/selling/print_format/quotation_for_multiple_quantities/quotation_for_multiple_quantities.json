 "html": "{{ letter_head }}\n</br>\n</br>\n<html> \n      <body>\n            <table width=100%>\n              <tr width=100%>\n                  <td width=50%>\n                      <table width=100% style=\"line-height:1;\">\n                          <tr width=100%>\n                             <td width=30%><b>To</b></td>\n                             <td width=70%>{{ doc.customer or \" \" }}</td>\n                          </tr>\n                          <tr width=100%>\n                             <td width=30%><b>Address</b></td>\n                             <td width=70%>{{ doc.address_display or \" \" }}\n                          </tr>\n                      </table>\n                  </td>\n                  <td width=50%>\n                       <table width=100% style=\"line-height:1;\">\n                           <tr width=100%>\n                               <td width=30%><b>From:</b></td>\n                               <td width=70%>Mr. Eric Lee</td>\n                           </tr>\n                           <tr width=100%>\n                               <td width=30%><b>Quote Ref:</b></td>\n                               <td width=70%>{{ doc.name or \" \" }}</td> \n                           </tr>\n                           <tr width=100%>\n                               <td width=30%><b>Date:</b></td>\n                               <td width=70%>{{ doc.transaction_date or \" \" }}</td> \n                           </tr>\n                           <tr width=100%>\n                                 <td width=30%><b>Currency:</b></td>\n                                 <td width=70%>{{ doc.currency or \" \" }}</td>\n                           </tr>\n                                 <tr width=100%>\n                                 <td width=30% nowrap><b>Customer Ref:</b></td>\n                                 <td width=70%>{{ doc.customer_ref or \" \" }}</td>\n                           </tr>\n                       </table>\n                  </td>\n              </tr>\n          </table>\n</br>\n</br>\n         <table width=100% class=table-bordered>\n         <tr width=100%>\n              <th rowspan=\"2\" style=\"text-align:center;\">S/n</th>\n              <th rowspan=\"2\" style=\"text-align:center;\">Part Name</th>\n              <th rowspan=\"2\" style=\"text-align:center;\">Drawing No</th>\n             <th rowspan=\"2\" style=\"text-align:center;\">Part No</th>\n             <th rowspan=\"2\" style=\"text-align:center;\">Material Spec/Type</th>\n             <th rowspan=\"2\" style=\"text-align:center;\">UOM</th>\n             <th colspan=\"3\" style=\"text-align:center;\">Quantity</th>  \n         </tr>\n         <tr width=100%>\n                          {% set q1=doc.rq1.split(\" \") %}\n                           <th style=\"text-align:center;\">{{ q1[1] or \"\" }}</th>\n                           {% set q2=doc.rq2.split(\" \") %}\n                           <th style=\"text-align:center;\">{{ q2[1] or \"\" }}</th>\n                           {% set q3=doc.rq3.split(\" \") %}\n                           <th style=\"text-align:center;\">{{ q3[1] or \"\" }}</th>    \n          </tr>\n           \n          </tr>\n          \n          {%- for row in doc.multiple_quantity_item -%}\n           <tr width=100%>\n              <td style=\"text-align:center;\">{{ row.idx or \" \"  }}</td>\n              <td style=\"text-align:center;\">{{ row.item_name or \" \" }}</br>{{ row.description }}</td>\n              <td style=\"text-align:center;\">{{ row.item_code or \" \" }}</td>\n             <td style=\"text-align:center;\">{{ row.part_number or \" \" }}</td>\n             <td style=\"text-align:center;\">{{ row.material_type or \" \" }}</td>\n             <td style=\"text-align:center;\">{{ row.stock_uom or \" \" }}\n             <td class=\"text-center\">{{ frappe.db.get_value(\"Currency\", doc.currency, \"symbol\") }} {{ row.get_formatted(\"r_qty1\", doc) }}</td>\n             <td class=\"text-center\">{{ frappe.db.get_value(\"Currency\", doc.currency, \"symbol\") }} {{ row.get_formatted(\"r_qty2\", doc) }}</td>\n             <td class=\"text-center\">{{ frappe.db.get_value(\"Currency\", doc.currency, \"symbol\") }} {{ row.get_formatted(\"r_qty3\", doc) }}</td>\n          </tr>\n                 \n            \n          {%- endfor -%}\n      </table>\n\n{%-  if doc.terms -%}\n</br>\n</br>\n      <table width=100%>\n          <tr width=100%> \n                <th width=100%>Terms and Condition</th>\n          </tr>\n          <tr width=100%>\n                 <td width=100%>{{ doc.terms }}</td>\n          </tr>\n      </table>\n{%-  endif -%}\n</br>\n</br>\n     <table width=100%>\n           <tr width=100%><td width=100%>Yours faithfully</br></br></br>\n            Eric Lee</br><b>Sales Manager</b>\n           </td></tr>\n     </table>\n   \n     </body>\n</html>", 
