 "html": "<div class=\"letter-head\">\n{{ letter_head }}\n</div>\n<div class=\"print-heading\">\n  <h3> {{ doc.doctype }} </h3>\n</div>\n<div class=\"row\">\n  <div class=\"col-xs-6\">\n    <div class=\"row\">\n      <div class=\"col-xs-5 text-right\" ><b>To:</b></div>\n      <div class=\"col-xs-7\">{{ doc.customer or \" \" }}</div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-xs-5 text-right\"><b>Address:</b></div>\n      <div class=\"col-xs-7\">{% set testing = doc.address_display or \"\" %}{% set another = \"<br>\".join(testing.split('<br>')) %}{{ another }}</div>\n    </div>\n  </div>\n  <div class=\"col-xs-6\">\n    <div class=\"row\">\n      <div class=\"col-xs-5 text-right\"><b>Customer Ref:</b></div>\n      <div class=\"col-xs-7\">{{ doc.customer_ref_no or \" \" }}</div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-xs-5 text-right\"><b>From:</b></div>\n      <div class=\"col-xs-7\">Mr. Eric Lee</div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-xs-5 text-right\"><b>Quote Ref:</b></div>\n      <div class=\"col-xs-7\">{{ doc.name or \" \" }}</div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-xs-5 text-right\"><b>Date:</b></div>\n      <div class=\"col-xs-7\">{{ doc.get_formatted(\"transaction_date\",doc) }}</div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-xs-5 text-right\"><b>Currency:</b></div>\n      <div class=\"col-xs-7\">{{ doc.currency or \" \" }}</div>\n    </div>\n    \n  </div>\n</div>\n<br>  \n<div class=\"row\">\n  <div class=\"col-xs-12\">\n    <table class=\"table table-bordered\">\n      <tbody>\n        <tr>\n          <th style=\"text-align:center;\">S/N</th>\n          <th style=\"text-align:center;\">Part Name</th>\n          <th style=\"text-align:center;\">Drawing No</th>\n          <th style=\"text-align:center;\">Part No</th>\n          <th style=\"text-align:center;\">Material Spec/Type</th>\n          <th style=\"text-align:center;\">Qty</th>\n          <th style=\"text-align:center;\">UOM</th>\n          <th style=\"text-align:center;\">U/Price</th>\n          <th style=\"text-align:center;\">Total</th>\n        </tr>\n        {%- for row in doc.quotation_details -%}\n          <tr>\n              <td style=\"width:3%;text-align:left;\">{{ row.idx or \" \"  }}</td>\n              <td style=\"width:10%;text-align:left;\">{{ row.item_name or \" \" }}</br>{{ row.remarks or \"\" }}</td>\n              <td style=\"width:20%;text-align:left;\">{{ row.item_code or \" \" }}</td>\n              <td style=\"width:15%;text-align:left;\">{{ row.part_number or \" \" }}</td>\n              <td style=\"width:15%;text-align:left;\">{{ row.material_type or \" \" }}</td>\n              <td style=\"width:3%;text-align:left;\">{{ row.quantity or '0' }}</td>\n              <td style=\"width:3%;text-align:left;\">{{ row.stock_uom or \" \" }}</td>\n              <td style=\"width:17%;text-align:left;\"> {{ row.get_formatted(\"rate\", doc) or '0.00'  }}</td>\n              <td style=\"width:15%;text-align:left;\">{{ row.get_formatted(\"amount\", doc) or '0.00'  }}</td>\n          </tr>\n        {%- endfor -%}\n      </tbody>  \n    </table>   \n  </div>\n</div>\n<div class=\"row\">\n  <div class=\"col-xs-6\"></div>\n  <div class=\"col-xs-6\">\n    <div class=\"col-xs-5 text-right\"><b>Net Total</b></div>\n    <div class=\"col-xs-7 text-right\">{{ doc.get_formatted(\"net_total_export\") }}</div>\n  </div>\n</div>\n{% set count=1 %}\n{%- for row in doc.other_charges -%}\n<div class=\"row\">\n  <div class=\"col-xs-6\"></div>\n  <div class=\"col-xs-6\">\n    <div class=\"col-xs-5 text-right\"><b>{{ row.description }}</b></div>\n    <div class=\"col-xs-7 text-right\">{{ row.get_formatted(\"tax_amount\", doc) }}</div>\n  </div>\n</div>\n{% set count=count+1 %}\n{%- endfor %}  \n<div class=\"row\">\n  <div class=\"col-xs-6\"></div>\n  <div class=\"col-xs-6\">\n    <div class=\"col-xs-5 text-right\"><b>Grand Total</b></div>\n    <div class=\"col-xs-7 text-right\">{{ doc.get_formatted(\"grand_total_export\") }}</div>\n  </div>\n</div>\n<div class=\"row\">\n  <div class=\"col-xs-6\"></div>\n  <div class=\"col-xs-6\">\n    <div class=\"col-xs-5 text-right\"><b>In Words</b></div>\n    <div class=\"col-xs-7 text-right\">{{ doc.in_words_export }}</div>\n  </div>\n</div>\n{%- if doc.terms %}\n<div class=\"row\">\n  <div class=\"col-xs-12 text-left\"><b>Terms and Conditions</b></div>\n</div>\n<div class=\"row\">\n  <div class=\"col-xs-12 text-left\">{{ doc.terms }}</div>\n</div>\n{%- endif %}\n<div class=\"row\">\n  <div class=\"col-xs-6\">\n    <p>Yours faithfully</p>\n    <br><br>\n    <p> Eric Lee</br><b>Sales Manager</b></p>\n  </div>\n</div> ", 
